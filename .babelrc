{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions"
                    ]
                }
            }
        ],
        "react",
        "es2015",
        "stage-0",
        "stage-1",
        "stage-2",
        "stage-3"
    ],
    "plugins": [
        "add-module-exports",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-decorators-legacy"
    ]
}
